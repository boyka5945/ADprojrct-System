@model IEnumerable<Inventory_mvc.Models.Collection_Point>

@{
    ViewBag.Title = "Stationery Collection Point";
}

<h2>ListCollectionPoint</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.collectionPointName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.collectionPointName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.collectionPointID }) |
            @Html.ActionLink("Details", "Details", new { id=item.collectionPointID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.collectionPointID })
        </td>
    </tr>
}

</table>
